<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
        xmlns="http://java.sun.com/xml/ns/persistence/orm"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_z.xsd"
        version="2.0">
    <named-native-query
            name="Comment.findNickname"
            result-class="com.example.practice2.entity.Comment">
        <query>
            <![CDATA[
                select * from comment where nickname = :nickname
            ]]>
        </query>
    </named-native-query>
</entity-mappings>